{
  "create-pickup": {
    "handler": "index.handler",
    "role_name": "lambda-dynamodb-role",
    "zip_file": "../dist/pickups/createPickup.zip",
    "environment": "TABLE_NAME=Pickups,NODE_PATH=/opt/nodejs/node_modules",
    "managed_policies": [
      "arn:aws:iam::aws:policy/service-role/AWSLambdaBasicExecutionRole"
    ],
    "inline_policies": {
      "DynamoDBAccess": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "dynamodb:GetItem",
              "dynamodb:PutItem",
              "dynamodb:UpdateItem",
              "dynamodb:DeleteItem",
              "dynamodb:Query",
              "dynamodb:Scan"
            ],
            "Resource": "arn:aws:dynamodb:us-west-2:369547265320:table/Pickups"
          }
        ]
      }
    }
  },
