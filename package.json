{
  "name": "dumprun",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "format": "biome format .",
    "generate-types": "openapi-typescript ./api/dumprun_openapi.json -o ./src/schemas/apiSchema.d.ts",
    "generate-zod": "openapi-zod-client ./api/dumprun_openapi.json -o ./src/schemas/zodSchemas.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --fix",
    "prepare": "husky install",
    "pre-commit": "pnpm format && pnpm validate",
    "test": "vitest run src",
    "test-integration": "vitest run integration-tests",
    "validate": "pnpm lint && pnpm check-types && pnpm test"
  },
  "author": "Nathan Bamford",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.5.4",
    "biome": "^0.3.3",
    "glob": "^11.0.0",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "openapi-typescript": "^7.4.0",
    "openapi-zod-client": "^1.18.2",
    "prisma": "^5.19.1",
    "rollup-plugin-node-externals": "^7.1.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.5",
    "vitest": "^2.1.1",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "@zodios/core": "^10.9.6"
  }
}
